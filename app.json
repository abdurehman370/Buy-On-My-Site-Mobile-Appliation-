{
  "expo": {
    "name": "BuyOnMySite",
    "slug": "buy-on-mysite",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "buyonmysite",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "android": {
      "package": "com.anonymous.buyonmysite",
      "intentFilters": [
        {
          "action": "SEND",
          "category": [
            "DEFAULT"
          ],
          "data": {
            "mimeType": "text/plain"
          }
        },
        {
          "action": "VIEW",
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ],
          "data": {
            "scheme": "https",
            "host": "*.homedepot.com"
          }
        }
      ]
    },
    "plugins": [
      "expo-router"
    ],
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "bb7b1bbb-925b-420b-bb21-3f3a81f4a36a"
      }
    }
  }
}